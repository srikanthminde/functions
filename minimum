<html>
    <head></head>
    <body>
        <script>
            function max (a,b){
                var max 
                var min
                if(a<b){
                    max = a
                    min = b
                }
                return [max,min]
            }
            var d=max(10,40)
            console.log(d)
           
        </script>
    </body>
</html>
