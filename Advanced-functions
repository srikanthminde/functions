            function add (a,b){
                var x= a+b
                return x
            }
            var d = add (50,20)
            console.log(d)

            var sub = function (a,b)
            {
                var y =a-b
                return y
            }
            var res2 =sub(49,9)
            console.log(res2)

            var sub = function (z,y){
                var p= z-y
                return p
            }
            var res3 = sub(50,10)
            console.log(res3)
            var sub = function (m,n)
            {
                var c= m-n
                return c
            }
            var w= sub(70,20)
            console.log(w)

